<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tiringbring.dailyexpenses">
    <uses-permission android:name="com.android.alarm.permission.SET_ALARM"/>
    <application
        android:configChanges="locale"
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logoround"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartActivity"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">

        </activity>
        <activity android:name=".AddExpense"
            android:exported="true"
            android:label="Add Expense"
            android:parentActivityName=".StartActivity"/>
        <activity android:name=".ExpenseList"
            android:exported="true"
            android:label="Expenses"
            android:parentActivityName=".StartActivity"/>
        <activity android:name=".SettingActivity"
            android:label="Settings"
            android:parentActivityName=".StartActivity"/>
        <receiver android:name=".Notification.Notification"
            android:process=":remote" />
        <meta-data android:name="com.onesignal.NotificationOpened.DEFAULT" android:value="DISABLE" />
    </application>

</manifest>